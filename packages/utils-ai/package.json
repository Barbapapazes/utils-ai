{
  "name": "utils-ai",
  "type": "module",
  "version": "0.1.5",
  "packageManager": "pnpm@8.9.0",
  "description": "A CLI of AI Utils",
  "author": "Est√©ban Soubiran <e.soubiran25@gmail.com> (https://github.com/barbapapazes/)",
  "license": "MIT",
  "homepage": "https://github.com/barbapapazes/utils-ai",
  "keywords": [
    "utils",
    "ai",
    "cli"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./cli": "./bin/index.mjs"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "bin": "./bin/index.mjs",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "dev": "jiti src/.ts",
    "build": "unbuild",
    "build:stub": "unbuild --stub",
    "start": "node bin/index.mjs",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "citty": "^0.1.4",
    "consola": "^3.2.3",
    "ofetch": "^1.3.3",
    "rc9": "^2.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.4",
    "jiti": "^1.20.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0"
  }
}
