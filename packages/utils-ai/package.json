{
  "name": "utils-ai",
  "type": "module",
  "version": "0.4.6",
  "packageManager": "pnpm@9.1.0",
  "description": "A CLI of AI Utils for writers",
  "author": "Est√©ban Soubiran <e.soubiran25@gmail.com> (https://github.com/barbapapazes/)",
  "license": "MIT",
  "homepage": "https://github.com/barbapapazes/utils-ai",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/barbapapazes/utils-ai.git",
    "directory": "packages/utils-ai"
  },
  "bugs": {
    "url": "https://github.com/barbapapazes/utils-ai/issues"
  },
  "keywords": [
    "utils",
    "ai",
    "cli"
  ],
  "exports": {
    ".": "./build/index.js"
  },
  "main": "build/index.js",
  "bin": "./bin/index.mjs",
  "files": [
    "!build/bin/test.js",
    "bin/index.mjs",
    "build/bin",
    "build/commands",
    "build/index.d.ts",
    "build/index.js",
    "build/src"
  ],
  "scripts": {
    "clean": "del-cli build",
    "typecheck": "tsc --noEmit",
    "build": "tsc",
    "prepublishOnly": "pnpm run build",
    "test": "c8 node --loader ts-node/esm --enable-source-maps bin/test.ts"
  },
  "dependencies": {
    "citty": "^0.1.4",
    "defu": "^6.1.2",
    "rc9": "^2.1.1"
  },
  "devDependencies": {
    "@japa/assert": "^3.0.0",
    "@japa/runner": "^3.1.4",
    "@swc/core": "^1.5.0",
    "@types/node": "^20.8.7",
    "c8": "^10.1.2",
    "del-cli": "^5.1.0",
    "jiti": "^1.20.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  },
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**",
      "tmp/**",
      "bin/**"
    ]
  },
  "imports": {
    "#package.json": "./package.json"
  }
}
